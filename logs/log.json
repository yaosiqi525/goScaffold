{
  "console": {
    "enable": true,
    "level": "FINEST",
    "pattern": "[%D %T] [%L] (%S) %M"
  },
  "files": [
    {
      "enable": true,
      "level": "DEBUG",
      "filename": "./logs/log.log",
      "category": "Test",
      "pattern": "[%D %T] [%L] (%S) %M",
      "rotate": true,
      "maxsize": "500M",
      "maxlines": "10K",
      "daily": true,
      "sanitize": true
    }
  ]
}